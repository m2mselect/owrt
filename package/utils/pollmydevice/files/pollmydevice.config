config interface '0'
        option mode 'disabled'
        option devicename '/dev/com1'
        option baudrate '9600'
        option bytesize '8'
        option parity 'none'
        option flowcontrol 'none'
        option stopbits '1'
        option server_port '33000'
        option holdconntime '60'
        option client_port '6008'
        option client_host ''
        option client_auth '1'
        option client_timeout '600'
        option modbus_gateway '0'

config interface '1'
        option mode 'disabled'
        option devicename '/dev/com0'
        option baudrate '9600'
        option bytesize '8'
        option parity 'none'
        option flowcontrol 'none'
        option stopbits '1'
        option server_port '33001'
        option holdconntime '60'
        option client_port '6008'
        option client_host ''
        option client_auth '1'
        option client_timeout '600'
        option modbus_gateway '0'
